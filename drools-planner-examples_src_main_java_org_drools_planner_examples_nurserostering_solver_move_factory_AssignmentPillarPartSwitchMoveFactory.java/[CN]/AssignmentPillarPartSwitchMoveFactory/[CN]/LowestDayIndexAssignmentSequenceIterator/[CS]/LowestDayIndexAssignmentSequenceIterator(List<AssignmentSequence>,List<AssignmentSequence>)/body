{
  leftIterator=leftAssignmentList.iterator();
  if (leftIterator.hasNext()) {
    nextLeft=leftIterator.next();
  }
 else {
    leftHasNext=false;
    nextLeft=null;
  }
  rightIterator=rightAssignmentList.iterator();
  if (rightIterator.hasNext()) {
    nextRight=rightIterator.next();
  }
 else {
    rightHasNext=false;
    nextRight=null;
  }
}
