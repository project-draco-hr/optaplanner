{
  assertEquals(SimpleDoubleScore.valueOfInitialized(2.25),SimpleDoubleScore.valueOfInitialized(1.5).power(2.0));
  assertEquals(SimpleDoubleScore.valueOfInitialized(1.5),SimpleDoubleScore.valueOfInitialized(2.25).power(0.5));
  assertEquals(SimpleDoubleScore.valueOf(-343,125.0),SimpleDoubleScore.valueOf(-7,5.0).power(3.0));
}
