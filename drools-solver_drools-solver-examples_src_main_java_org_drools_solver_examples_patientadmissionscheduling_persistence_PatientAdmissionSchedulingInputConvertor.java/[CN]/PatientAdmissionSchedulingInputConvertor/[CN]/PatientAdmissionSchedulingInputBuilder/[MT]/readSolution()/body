{
  patientAdmissionSchedule=new PatientAdmissionSchedule();
  patientAdmissionSchedule.setId(0L);
  readSizes();
  readEmptyLine();
  readEmptyLine();
  readSpecialismList();
  readEmptyLine();
  readDepartmentListAndDepartmentSpecialismList();
  readEmptyLine();
  readEquipmentList();
  readEmptyLine();
  return patientAdmissionSchedule;
}
