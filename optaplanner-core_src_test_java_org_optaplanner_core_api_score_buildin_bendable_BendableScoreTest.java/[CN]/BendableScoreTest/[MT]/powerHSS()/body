{
  assertEquals(scoreDefinitionHSS.createScoreInitialized(9,16,25),scoreDefinitionHSS.createScoreInitialized(3,-4,5).power(2.0));
  assertEquals(scoreDefinitionHSS.createScoreInitialized(3,4,5),scoreDefinitionHSS.createScoreInitialized(9,16,25).power(0.5));
  assertEquals(scoreDefinitionHSS.createScore(-343,27,-64,125),scoreDefinitionHSS.createScore(-7,3,-4,5).power(3.0));
}
