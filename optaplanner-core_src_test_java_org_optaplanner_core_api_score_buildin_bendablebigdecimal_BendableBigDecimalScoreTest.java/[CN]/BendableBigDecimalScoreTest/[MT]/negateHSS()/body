{
  assertEquals(scoreDefinitionHSS.createScoreInitialized(MINUS_THREE,FOUR,MINUS_FIVE),scoreDefinitionHSS.createScoreInitialized(THREE,MINUS_FOUR,FIVE).negate());
  assertEquals(scoreDefinitionHSS.createScoreInitialized(THREE,MINUS_FOUR,FIVE),scoreDefinitionHSS.createScoreInitialized(MINUS_THREE,FOUR,MINUS_FIVE).negate());
}
