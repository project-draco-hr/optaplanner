{
  assertEquals(scoreDefinitionHSS.createScoreInitialized(PLUS_19,MINUS_320,ZERO),scoreDefinitionHSS.createScoreInitialized(PLUS_20,MINUS_20,MINUS_4000).add(scoreDefinitionHSS.createScoreInitialized(MINUS_ONE,MINUS_300,PLUS_4000)));
  assertEquals(scoreDefinitionHSS.createScore(-77,PLUS_19,MINUS_320,ZERO),scoreDefinitionHSS.createScore(-70,PLUS_20,MINUS_20,MINUS_4000).add(scoreDefinitionHSS.createScore(-7,MINUS_ONE,MINUS_300,PLUS_4000)));
}
