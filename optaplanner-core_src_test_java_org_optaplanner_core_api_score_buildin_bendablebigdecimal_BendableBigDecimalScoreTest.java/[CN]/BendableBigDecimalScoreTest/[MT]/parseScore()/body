{
  assertEquals(scoreDefinitionHSS.createScoreInitialized(BigDecimal.valueOf(-147),BigDecimal.valueOf(-258),BigDecimal.valueOf(-369)),scoreDefinitionHSS.parseScore("[-147]hard/[-258/-369]soft"));
  assertEquals(scoreDefinitionHSS.createScore(-7,BigDecimal.valueOf(-147),BigDecimal.valueOf(-258),BigDecimal.valueOf(-369)),scoreDefinitionHSS.parseScore("-7init/[-147]hard/[-258/-369]soft"));
}
