{
  GroupLayout layout=new GroupLayout(this);
  setLayout(layout);
  createEmployeeListPanel();
  JPanel headerPanel=new JPanel();
  layout.setHorizontalGroup(layout.createParallelGroup().addComponent(headerPanel).addComponent(employeeListPanel));
