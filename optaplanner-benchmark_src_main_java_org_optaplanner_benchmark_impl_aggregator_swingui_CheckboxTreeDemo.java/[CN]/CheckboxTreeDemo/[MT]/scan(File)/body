{
  CustomCheckbox checkbox=new CustomCheckbox(file.getName());
  DefaultMutableTreeNode treeNde=new DefaultMutableTreeNode(checkbox);
  if (file.isDirectory()) {
    for (    File child : file.listFiles()) {
      MutableTreeNode nod=scan(child);
      if (nod != null) {
        treeNde.add(nod);
      }
    }
  }
  return treeNde;
}
