{
  assertEquals(HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("-147.2"),new BigDecimal("-258.3")),HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("-147.2"),new BigDecimal("-258.3")).toInitializedScore());
  assertEquals(HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("-147.2"),new BigDecimal("-258.3")),HardSoftBigDecimalScore.valueOf(-7,new BigDecimal("-147.2"),new BigDecimal("-258.3")).toInitializedScore());
}
