{
  assertEquals(HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("16.0"),new BigDecimal("25.0")),HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("-4.0"),new BigDecimal("5.0")).power(2.0));
  assertEquals(HardSoftBigDecimalScore.valueOf(-343,new BigDecimal("-64.0"),new BigDecimal("125.0")),HardSoftBigDecimalScore.valueOf(-7,new BigDecimal("-4.0"),new BigDecimal("5.0")).power(3.0));
}
