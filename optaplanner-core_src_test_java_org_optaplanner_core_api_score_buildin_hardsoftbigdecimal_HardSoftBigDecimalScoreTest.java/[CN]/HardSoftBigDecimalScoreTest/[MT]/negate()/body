{
  assertEquals(HardSoftBigDecimalScore.valueOf(new BigDecimal("-4.0"),new BigDecimal("5.0")),HardSoftBigDecimalScore.valueOf(new BigDecimal("4.0"),new BigDecimal("-5.0")).negate());
  assertEquals(HardSoftBigDecimalScore.valueOf(new BigDecimal("4.0"),new BigDecimal("-5.0")),HardSoftBigDecimalScore.valueOf(new BigDecimal("-4.0"),new BigDecimal("5.0")).negate());
}
