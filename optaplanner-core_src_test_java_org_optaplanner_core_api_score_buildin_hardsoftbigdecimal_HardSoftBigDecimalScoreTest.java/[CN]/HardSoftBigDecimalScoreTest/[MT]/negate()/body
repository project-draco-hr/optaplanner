{
  assertEquals(HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("-4.0"),new BigDecimal("5.0")),HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("4.0"),new BigDecimal("-5.0")).negate());
  assertEquals(HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("4.0"),new BigDecimal("-5.0")),HardSoftBigDecimalScore.valueOfInitialized(new BigDecimal("-4.0"),new BigDecimal("5.0")).negate());
}
