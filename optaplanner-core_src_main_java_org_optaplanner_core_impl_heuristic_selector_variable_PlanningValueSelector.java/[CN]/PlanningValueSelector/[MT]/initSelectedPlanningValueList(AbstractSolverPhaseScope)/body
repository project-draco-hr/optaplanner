{
  if (valueRangeDescriptor.isEntityIndependent()) {
    CountableValueRange<?> valueRange=(CountableValueRange<?>)((EntityIndependentPlanningValueRangeDescriptor)valueRangeDescriptor).extractValueRange(phaseScope.getWorkingSolution());
    cachedPlanningValues=IteratorUtils.toList(valueRange.createOriginalIterator(),(int)valueRange.getSize());
  }
 else {
    cachedPlanningValues=null;
  }
}
