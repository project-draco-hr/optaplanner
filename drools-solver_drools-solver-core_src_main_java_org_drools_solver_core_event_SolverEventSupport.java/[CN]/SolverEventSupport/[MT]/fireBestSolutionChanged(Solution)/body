{
  final Iterator<SolverEventListener> iter=getEventListenersIterator();
  if (iter.hasNext()) {
    final BestSolutionChangedEvent event=new BestSolutionChangedEvent(solver,solver.getTimeMillisSpend(),newBestSolution);
    do {
      iter.next().bestSolutionChanged(event);
    }
 while (iter.hasNext());
  }
}
