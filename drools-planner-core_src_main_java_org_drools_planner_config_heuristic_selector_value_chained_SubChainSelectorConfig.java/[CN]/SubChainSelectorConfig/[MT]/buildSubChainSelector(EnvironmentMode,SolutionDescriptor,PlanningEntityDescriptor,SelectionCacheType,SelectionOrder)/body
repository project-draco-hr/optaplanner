{
  if (minimumCacheType.compareTo(SelectionCacheType.STEP) > 0) {
    throw new IllegalArgumentException("The subChainSelectorConfig (" + this + ")'s minimumCacheType ("+ minimumCacheType+ ") must not be higher than "+ SelectionCacheType.STEP+ " because the chains change every step.");
  }
  ValueSelector valueSelector=valueSelectorConfig.buildValueSelector(environmentMode,solutionDescriptor,entityDescriptor,minimumCacheType,SelectionOrder.ORIGINAL);
  return new DefaultSubChainSelector(valueSelector,inheritedSelectionOrder.toRandomSelectionBoolean(),minimumSubChainSize == null ? DEFAULT_MINIMUM_SUB_CHAIN_SIZE : minimumSubChainSize,maximumSubChainSize == null ? DEFAULT_MAXIMUM_SUB_CHAIN_SIZE : maximumSubChainSize);
}
