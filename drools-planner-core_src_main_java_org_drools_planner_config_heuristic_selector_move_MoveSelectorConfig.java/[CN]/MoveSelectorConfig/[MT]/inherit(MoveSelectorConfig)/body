{
  super.inherit(inheritedConfig);
  selectionOrder=ConfigUtils.inheritOverwritableProperty(selectionOrder,inheritedConfig.getSelectionOrder());
  cacheType=ConfigUtils.inheritOverwritableProperty(cacheType,inheritedConfig.getCacheType());
  moveFilterClassList=ConfigUtils.inheritOverwritableProperty(moveFilterClassList,inheritedConfig.getMoveFilterClassList());
  moveProbabilityWeightFactoryClass=ConfigUtils.inheritOverwritableProperty(moveProbabilityWeightFactoryClass,inheritedConfig.getMoveProbabilityWeightFactoryClass());
  fixedProbabilityWeight=ConfigUtils.inheritOverwritableProperty(fixedProbabilityWeight,inheritedConfig.getFixedProbabilityWeight());
}
