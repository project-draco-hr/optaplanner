{
  assertEquals(scoreDefinitionHHSSS.createScoreInitialized(40000000000L,160000000000L,250000000000L,0L,0L),scoreDefinitionHHSSS.createScoreInitialized(200000L,-400000L,500000L,0L,0L).power(2.0));
  assertEquals(scoreDefinitionHHSSS.createScoreInitialized(3L,4L,5L,0L,0L),scoreDefinitionHHSSS.createScoreInitialized(9L,16L,25L,0L,0L).power(0.5));
}
