{
  assertEquals(scoreDefinitionHSS.createScoreInitialized(-3000000000L,4000000000L,-5000000000L),scoreDefinitionHSS.createScoreInitialized(3000000000L,-4000000000L,5000000000L).negate());
  assertEquals(scoreDefinitionHSS.createScoreInitialized(3000000000L,-4000000000L,5000000000L),scoreDefinitionHSS.createScoreInitialized(-3000000000L,4000000000L,-5000000000L).negate());
}
