{
  assertEquals(scoreDefinitionHHSSS.createScore(5000000000L,-5000000000L,5000000000L,0,0),scoreDefinitionHHSSS.createScore(25000000000L,-25000000000L,25000000000L,0,0).divide(5.0));
  assertEquals(scoreDefinitionHHSSS.createScore(4,-5,4,0,0),scoreDefinitionHHSSS.createScore(21,-21,21,0,0).divide(5.0));
  assertEquals(scoreDefinitionHHSSS.createScore(4,-5,4,0,0),scoreDefinitionHHSSS.createScore(24,-24,24,0,0).divide(5.0));
}
