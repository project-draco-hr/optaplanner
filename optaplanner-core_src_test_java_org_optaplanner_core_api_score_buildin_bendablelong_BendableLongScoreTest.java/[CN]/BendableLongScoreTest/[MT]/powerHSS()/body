{
  assertEquals(scoreDefinitionHSS.createScoreInitialized(90000000000L,160000000000L,250000000000L),scoreDefinitionHSS.createScoreInitialized(300000L,-400000L,500000L).power(2.0));
  assertEquals(scoreDefinitionHSS.createScoreInitialized(300000L,400000L,500000L),scoreDefinitionHSS.createScoreInitialized(90000000000L,160000000000L,250000000000L).power(0.5));
  assertEquals(scoreDefinitionHSS.createScore(-343,27L,-64L,125L),scoreDefinitionHSS.createScore(-7,3L,-4L,5L).power(3.0));
}
