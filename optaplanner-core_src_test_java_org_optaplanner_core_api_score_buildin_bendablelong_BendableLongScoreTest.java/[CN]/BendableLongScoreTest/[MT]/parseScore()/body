{
  assertEquals(scoreDefinitionHSS.createScoreInitialized(-5432109876L,-9876543210L,-3456789012L),scoreDefinitionHSS.parseScore("[-5432109876]hard/[-9876543210/-3456789012]soft"));
  assertEquals(scoreDefinitionHSS.createScore(-7,-5432109876L,-9876543210L,-3456789012L),scoreDefinitionHSS.parseScore("-7init/[-5432109876]hard/[-9876543210/-3456789012]soft"));
}
