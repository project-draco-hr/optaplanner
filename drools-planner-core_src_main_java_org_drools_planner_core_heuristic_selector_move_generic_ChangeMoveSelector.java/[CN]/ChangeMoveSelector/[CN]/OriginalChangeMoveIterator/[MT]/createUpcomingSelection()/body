{
  while (!valueIterator.hasNext(upcomingEntity)) {
    if (!entityIterator.hasNext()) {
      upcomingSelection=null;
      return;
    }
    upcomingEntity=entityIterator.next();
    valueIterator=valueSelector.iterator();
  }
  Object toValue=valueIterator.next(upcomingEntity);
  upcomingSelection=chained ? new GenericChainedChangeMove(upcomingEntity,valueSelector.getVariableDescriptor(),toValue) : new GenericChangeMove(upcomingEntity,valueSelector.getVariableDescriptor(),toValue);
}
