{
  for (  Period period : examination.getPeriodList()) {
    roomsPanel.addRowHeader(HEADER_COLUMN,period,createHeaderPanel(new JLabel(period.getLabel(),new PeriodIcon(period),SwingConstants.LEFT),"Duration: " + period.getDuration()));
  }
  roomsPanel.addRowHeader(HEADER_COLUMN,null,createHeaderPanel(new JLabel("Unassigned"),null));
}
