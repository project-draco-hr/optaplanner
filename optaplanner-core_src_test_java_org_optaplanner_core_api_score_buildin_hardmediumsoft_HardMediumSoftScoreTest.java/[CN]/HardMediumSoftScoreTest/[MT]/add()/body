{
  assertEquals(HardMediumSoftScore.valueOfInitialized(19,-320,0),HardMediumSoftScore.valueOfInitialized(20,-20,-4000).add(HardMediumSoftScore.valueOfInitialized(-1,-300,4000)));
  assertEquals(HardMediumSoftScore.valueOf(-77,19,-320,0),HardMediumSoftScore.valueOf(-70,20,-20,-4000).add(HardMediumSoftScore.valueOf(-7,-1,-300,4000)));
}
