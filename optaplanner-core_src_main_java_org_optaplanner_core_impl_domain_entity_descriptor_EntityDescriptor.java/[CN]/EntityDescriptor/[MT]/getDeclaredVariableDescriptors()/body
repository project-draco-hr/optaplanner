{
  Collection<VariableDescriptor> variableDescriptors=new ArrayList<VariableDescriptor>(declaredGenuineVariableDescriptorMap.size() + declaredShadowVariableDescriptorMap.size());
  variableDescriptors.addAll(declaredGenuineVariableDescriptorMap.values());
  variableDescriptors.addAll(declaredShadowVariableDescriptorMap.values());
  return variableDescriptors;
}
