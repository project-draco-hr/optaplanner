{
  failureCount=0;
  totalScore=null;
  for (  SingleBenchmark singleBenchmark : singleBenchmarkList) {
    if (singleBenchmark.isFailure()) {
      failureCount++;
    }
 else {
      if (totalScore == null) {
        totalScore=singleBenchmark.getScore();
      }
 else {
        totalScore=totalScore.add(singleBenchmark.getScore());
      }
    }
  }
}
