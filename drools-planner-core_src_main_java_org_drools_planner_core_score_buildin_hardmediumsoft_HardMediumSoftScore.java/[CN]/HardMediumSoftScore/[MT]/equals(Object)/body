{
  if (this == o) {
    return true;
  }
 else   if (o instanceof HardMediumSoftScore) {
    HardMediumSoftScore other=(HardMediumSoftScore)o;
    return hardScore == other.getHardScore() && mediumScore == other.getMediumScore() && softScore == other.getSoftScore();
  }
 else {
    return false;
  }
}
