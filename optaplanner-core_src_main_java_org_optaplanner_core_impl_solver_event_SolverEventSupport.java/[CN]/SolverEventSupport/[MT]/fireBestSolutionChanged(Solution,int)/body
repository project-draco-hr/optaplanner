{
  final Iterator<SolverEventListener> it=eventListenerSet.iterator();
  if (it.hasNext()) {
    final BestSolutionChangedEvent event=new BestSolutionChangedEvent(solver,solver.getSolverScope().calculateTimeMillisSpent(),newBestSolution,newUninitializedVariableCount);
    do {
      it.next().bestSolutionChanged(event);
    }
 while (it.hasNext());
  }
}
