{
  travelingSalesmanSchedule=new TravelingSalesmanSchedule();
  travelingSalesmanSchedule.setId(0L);
  readHeaders();
  readCityList();
  readConstantLine("EOF");
  logger.info("TravelingSalesmanSchedule with {} cities.",travelingSalesmanSchedule.getCityList().size());
  BigInteger possibleSolutionSize=factorial(travelingSalesmanSchedule.getCityList().size() - 1);
  String flooredPossibleSolutionSize="10^" + (possibleSolutionSize.toString().length() - 1);
  logger.info("TravelingSalesmanSchedule with flooredPossibleSolutionSize ({}) and possibleSolutionSize({}).",flooredPossibleSolutionSize,possibleSolutionSize);
  return travelingSalesmanSchedule;
}
