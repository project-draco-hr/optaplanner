{
  travelingSalesmanTour=new TravelingSalesmanTour();
  travelingSalesmanTour.setId(0L);
  readHeaders();
  readCityList();
  readConstantLine("EOF");
  createJourneyList();
  logger.info("TravelingSalesmanTour with {} cities.",travelingSalesmanTour.getCityList().size());
  BigInteger possibleSolutionSize=factorial(travelingSalesmanTour.getCityList().size() - 1);
  String flooredPossibleSolutionSize="10^" + (possibleSolutionSize.toString().length() - 1);
  logger.info("TravelingSalesmanTour with flooredPossibleSolutionSize ({}) and possibleSolutionSize ({}).",flooredPossibleSolutionSize,possibleSolutionSize);
  return travelingSalesmanTour;
}
