{
  Long id=persistAndAssert(new TestJpaEntity(null));
  findAssertAndChangeScore(TestJpaEntity.class,id,null,HardSoftBigDecimalScore.valueOf(new BigDecimal("-10.01000"),new BigDecimal("-2.20000")));
  findAndAssert(TestJpaEntity.class,id,HardSoftBigDecimalScore.valueOf(new BigDecimal("-10.01000"),new BigDecimal("-2.20000")));
}
