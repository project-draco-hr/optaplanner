{
  GenuineVariableDescriptor variableDescriptor=((InnerScoreDirector)guiScoreDirector).getSolutionDescriptor().findGenuineVariableDescriptorOrFail(entity,variableName);
  if (variableDescriptor.isChained()) {
    return new ChainedChangeMove(entity,variableDescriptor,toPlanningValue);
  }
 else {
    return new ChangeMove(entity,variableDescriptor,toPlanningValue);
  }
}
