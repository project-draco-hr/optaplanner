{
  if (this == o) {
    return true;
  }
 else   if (o instanceof HardSoftBigDecimalScore) {
    HardSoftBigDecimalScore other=(HardSoftBigDecimalScore)o;
    return initScore == other.getInitScore() && hardScore.equals(other.getHardScore()) && softScore.equals(other.getSoftScore());
  }
 else {
    return false;
  }
}
