{
  assertEquals(HardSoftScore.valueOfInitialized(5,-5),HardSoftScore.valueOfInitialized(25,-25).divide(5.0));
  assertEquals(HardSoftScore.valueOfInitialized(4,-5),HardSoftScore.valueOfInitialized(21,-21).divide(5.0));
  assertEquals(HardSoftScore.valueOfInitialized(4,-5),HardSoftScore.valueOfInitialized(24,-24).divide(5.0));
  assertEquals(HardSoftScore.valueOf(-7,4,-5),HardSoftScore.valueOf(-14,8,-10).divide(2.0));
}
