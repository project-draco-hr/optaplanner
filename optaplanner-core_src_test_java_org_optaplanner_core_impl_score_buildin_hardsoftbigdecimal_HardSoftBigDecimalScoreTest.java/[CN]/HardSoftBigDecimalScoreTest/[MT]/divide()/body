{
  assertEquals(HardSoftBigDecimalScore.valueOf(new BigDecimal("5.0"),new BigDecimal("-5.0")),HardSoftBigDecimalScore.valueOf(new BigDecimal("25.0"),new BigDecimal("-25.0")).divide(5.0));
  assertEquals(HardSoftBigDecimalScore.valueOf(new BigDecimal("4.2"),new BigDecimal("-4.2")),HardSoftBigDecimalScore.valueOf(new BigDecimal("21.0"),new BigDecimal("-21.0")).divide(5.0));
  assertEquals(HardSoftBigDecimalScore.valueOf(new BigDecimal("4.8"),new BigDecimal("-4.8")),HardSoftBigDecimalScore.valueOf(new BigDecimal("24.0"),new BigDecimal("-24.0")).divide(5.0));
}
