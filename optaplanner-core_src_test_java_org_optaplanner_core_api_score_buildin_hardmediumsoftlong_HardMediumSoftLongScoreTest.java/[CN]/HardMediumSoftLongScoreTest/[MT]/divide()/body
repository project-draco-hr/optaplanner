{
  assertEquals(HardMediumSoftLongScore.valueOf(5L,-5L,5L),HardMediumSoftLongScore.valueOf(25L,-25L,25L).divide(5.0));
  assertEquals(HardMediumSoftLongScore.valueOf(4L,-5L,4L),HardMediumSoftLongScore.valueOf(21L,-21L,21L).divide(5.0));
  assertEquals(HardMediumSoftLongScore.valueOf(4L,-5L,4L),HardMediumSoftLongScore.valueOf(24L,-24L,24L).divide(5.0));
}
