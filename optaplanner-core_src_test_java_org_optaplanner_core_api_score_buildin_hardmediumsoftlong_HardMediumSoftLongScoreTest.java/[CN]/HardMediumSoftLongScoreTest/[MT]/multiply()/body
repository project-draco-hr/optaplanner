{
  assertEquals(HardMediumSoftLongScore.valueOf(6L,-6L,6L),HardMediumSoftLongScore.valueOf(5L,-5L,5L).multiply(1.2));
  assertEquals(HardMediumSoftLongScore.valueOf(1L,-2L,1L),HardMediumSoftLongScore.valueOf(1L,-1L,1L).multiply(1.2));
  assertEquals(HardMediumSoftLongScore.valueOf(4L,-5L,4L),HardMediumSoftLongScore.valueOf(4L,-4L,4L).multiply(1.2));
}
