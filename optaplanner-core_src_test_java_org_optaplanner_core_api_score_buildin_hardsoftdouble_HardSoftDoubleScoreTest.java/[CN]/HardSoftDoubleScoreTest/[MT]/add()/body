{
  assertEquals(HardSoftDoubleScore.valueOfInitialized(19.0,-320.0),HardSoftDoubleScore.valueOfInitialized(20.0,-20.0).add(HardSoftDoubleScore.valueOfInitialized(-1.0,-300.0)));
  assertEquals(HardSoftDoubleScore.valueOf(-77,19.0,-320.0),HardSoftDoubleScore.valueOf(-70,20.0,-20.0).add(HardSoftDoubleScore.valueOf(-7,-1.0,-300.0)));
}
