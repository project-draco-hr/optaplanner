{
  assertEquals(HardSoftDoubleScore.valueOfInitialized(5.0,-5.0),HardSoftDoubleScore.valueOfInitialized(25.0,-25.0).divide(5.0));
  assertEquals(HardSoftDoubleScore.valueOfInitialized(4.2,-4.2),HardSoftDoubleScore.valueOfInitialized(21.0,-21.0).divide(5.0));
  assertEquals(HardSoftDoubleScore.valueOfInitialized(4.8,-4.8),HardSoftDoubleScore.valueOfInitialized(24.0,-24.0).divide(5.0));
  assertEquals(HardSoftDoubleScore.valueOf(-7,4.3,-5.2),HardSoftDoubleScore.valueOf(-14,8.6,-10.4).divide(2.0));
}
