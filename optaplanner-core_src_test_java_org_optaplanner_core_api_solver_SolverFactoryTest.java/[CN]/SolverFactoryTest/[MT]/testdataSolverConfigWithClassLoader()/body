{
  ClassLoader classLoader=new DivertingClassLoader(getClass().getClassLoader());
  SolverFactory solverFactory=SolverFactory.createFromXmlResource("divertThroughClassLoader/org/optaplanner/core/api/solver/classloaderTestdataSolverConfig.xml",classLoader);
  Solver solver=solverFactory.buildSolver();
  assertNotNull(solver);
}
