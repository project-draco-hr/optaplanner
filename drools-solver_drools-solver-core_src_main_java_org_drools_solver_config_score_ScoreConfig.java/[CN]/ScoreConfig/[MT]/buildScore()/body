{
  if (score != null) {
    return score;
  }
 else   if (scoreType != null) {
switch (scoreType) {
case SIMPLE:
      return DefaultSimpleScore.parseScore(scoreString);
case HARD_AND_SOFT:
    return DefaultHardAndSoftScore.parseScore(scoreString);
default :
  throw new IllegalStateException("The scoreType (" + scoreType + ") is not implemented");
}
}
 else {
return null;
}
}
