{
  trainDesign=new TrainDesign();
  trainDesign.setId(0L);
  readRailNodeList();
  readCarBlockList();
  readRailArcList();
  readCrewSegmentList();
  readTrainDesignParametrization();
  trainDesign.initializeTransientProperties();
  logger.info("TrainDesign with {} rail nodes, {} rail arcs, {} car blocks, {} crew segments.",new Object[]{trainDesign.getRailNodeList().size(),trainDesign.getRailArcList().size(),trainDesign.getCarBlockList().size(),trainDesign.getCrewSegmentList().size()});
  return trainDesign;
}
