{
  trainDesign=new TrainDesign();
  trainDesign.setId(0L);
  readRailNodeList();
  readCarBlockList();
  readRailArcList();
  readTrainCrewList();
  readTrainDesignParametrization();
  buildTodo();
  logger.info("TrainDesign with {} rail nodes, {} rail arcs, {} car blocks, {} train crews.",new Object[]{trainDesign.getRailNodeList().size(),trainDesign.getRailArcList().size(),trainDesign.getCarBlockList().size(),trainDesign.getTrainCrewList().size()});
  return trainDesign;
}
