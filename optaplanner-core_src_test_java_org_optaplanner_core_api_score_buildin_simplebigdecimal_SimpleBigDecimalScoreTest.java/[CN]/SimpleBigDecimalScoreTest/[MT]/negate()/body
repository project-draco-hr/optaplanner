{
  assertEquals(SimpleBigDecimalScore.valueOfInitialized(new BigDecimal("-5.0")),SimpleBigDecimalScore.valueOfInitialized(new BigDecimal("5.0")).negate());
  assertEquals(SimpleBigDecimalScore.valueOfInitialized(new BigDecimal("5.0")),SimpleBigDecimalScore.valueOfInitialized(new BigDecimal("-5.0")).negate());
}
