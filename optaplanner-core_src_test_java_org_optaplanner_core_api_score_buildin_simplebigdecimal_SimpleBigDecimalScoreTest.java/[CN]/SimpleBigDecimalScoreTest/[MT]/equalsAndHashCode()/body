{
  assertScoresEqualsAndHashCode(SimpleBigDecimalScore.valueOfInitialized(new BigDecimal("-10.0")),SimpleBigDecimalScore.valueOfInitialized(new BigDecimal("-10.0")),SimpleBigDecimalScore.valueOf(0,new BigDecimal("-10.0")));
  assertScoresEqualsAndHashCode(SimpleBigDecimalScore.valueOf(-7,new BigDecimal("-10.0")),SimpleBigDecimalScore.valueOf(-7,new BigDecimal("-10.0")));
  assertScoresNotEquals(SimpleBigDecimalScore.valueOfInitialized(new BigDecimal("-10.0")),SimpleBigDecimalScore.valueOfInitialized(new BigDecimal("-30.0")),SimpleBigDecimalScore.valueOf(-7,new BigDecimal("-10.0")));
}
