{
  if (this == o) {
    return true;
  }
 else   if (o instanceof PatternEntryOccurrence) {
    PatternEntryOccurrence other=(PatternEntryOccurrence)o;
    return new EqualsBuilder().append(patternEntry,other.patternEntry).append(employee,other.employee).append(shiftDate,other.shiftDate).isEquals();
  }
 else {
    return false;
  }
}
