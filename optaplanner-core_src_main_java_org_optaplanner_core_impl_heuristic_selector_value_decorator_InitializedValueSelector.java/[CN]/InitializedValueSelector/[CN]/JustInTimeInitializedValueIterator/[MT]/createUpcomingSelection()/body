{
  Object next;
  long attemptsBeforeBailOut=bailOutEnabled ? determineBailOutSize(entity) : 0L;
  do {
    if (!childValueIterator.hasNext()) {
      return noUpcomingSelection();
    }
    if (bailOutEnabled) {
      if (attemptsBeforeBailOut <= 0L) {
        logger.warn("Bailing out of neverEnding selector ({}) to avoid infinite loop.",InitializedValueSelector.this);
        return noUpcomingSelection();
      }
      attemptsBeforeBailOut--;
    }
    next=childValueIterator.next();
  }
 while (!accept(next));
  return next;
}
