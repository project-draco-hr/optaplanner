{
  HardSoftScoreDefinition scoreDefinition=new HardSoftScoreDefinition();
  HardSoftScore optimisticBound=scoreDefinition.buildOptimisticBound(InitializingScoreTrend.buildUniformTrend(InitializingScoreTrendLevel.ONLY_DOWN,2),HardSoftScore.valueOf(-1,-2));
  assertEquals(-1,optimisticBound.getHardScore());
  assertEquals(-2,optimisticBound.getSoftScore());
}
