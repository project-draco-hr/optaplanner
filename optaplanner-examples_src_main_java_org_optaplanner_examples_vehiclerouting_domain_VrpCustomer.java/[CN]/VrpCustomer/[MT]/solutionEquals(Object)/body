{
  if (this == o) {
    return true;
  }
 else   if (o instanceof VrpCustomer) {
    VrpCustomer other=(VrpCustomer)o;
    return new EqualsBuilder().append(id,other.id).append(location,other.location).append(previousStandstill,other.previousStandstill).isEquals();
  }
 else {
    return false;
  }
}
