{
  if (betaDistributionAlpha <= 0) {
    throw new IllegalArgumentException("The betaDistributionAlpha (" + betaDistributionAlpha + ") must be positive.");
  }
  if (betaDistributionBeta <= 0) {
    throw new IllegalArgumentException("The betaDistributionBeta (" + betaDistributionBeta + ") must be positive.");
  }
  betaDistribution=new BetaDistribution(betaDistributionAlpha,betaDistributionBeta);
}
