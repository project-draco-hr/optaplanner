{
  long fromQuantity=fromAssetClassAllocation.getQuantityMillis();
  long toQuantity=toAssetClassAllocation.getQuantityMillis();
  return "[" + fromAssetClassAllocation + " {"+ fromQuantity+ "->"+ (fromQuantity - transferMillis)+ "}, "+ toAssetClassAllocation+ " {"+ toQuantity+ "->"+ (toQuantity + transferMillis)+ "}]";
}
