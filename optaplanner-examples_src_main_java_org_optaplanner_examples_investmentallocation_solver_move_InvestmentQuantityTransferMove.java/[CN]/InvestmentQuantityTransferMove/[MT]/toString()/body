{
  long fromQuantity=fromAssetClassAllocation.getQuantityMicros();
  long toQuantity=toAssetClassAllocation.getQuantityMicros();
  return "[" + fromAssetClassAllocation + " {"+ fromQuantity+ "->"+ (fromQuantity - transferMicros)+ "}, "+ toAssetClassAllocation+ " {"+ toQuantity+ "->"+ (toQuantity + transferMicros)+ "}]";
}
