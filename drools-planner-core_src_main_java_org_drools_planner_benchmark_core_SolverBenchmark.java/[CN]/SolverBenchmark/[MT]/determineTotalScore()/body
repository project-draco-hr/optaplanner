{
  failureCount=0;
  totalScore=null;
  for (  PlannerBenchmarkResult result : plannerBenchmarkResultList) {
    if (result.isFailure()) {
      failureCount++;
      continue;
    }
    if (totalScore == null) {
      totalScore=result.getScore();
    }
 else {
      totalScore=totalScore.add(result.getScore());
    }
  }
}
