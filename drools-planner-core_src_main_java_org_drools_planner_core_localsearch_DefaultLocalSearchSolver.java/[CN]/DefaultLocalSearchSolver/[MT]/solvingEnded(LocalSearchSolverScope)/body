{
  decider.solvingEnded(localSearchSolverScope);
  termination.solvingEnded(localSearchSolverScope);
  bestSolutionRecaller.solvingEnded(localSearchSolverScope);
  long timeMillisSpend=localSearchSolverScope.calculateTimeMillisSpend();
  long averageCalculateCountPerSecond=(timeMillisSpend == 0L) ? 0L : localSearchSolverScope.getCalculateCount() * 1000L / timeMillisSpend;
  logger.info("Solved at step index ({}) with time spend ({}) for best score ({})" + " with average calculate count per second ({}).",new Object[]{localSearchSolverScope.getLastCompletedLocalSearchStepScope().getStepIndex(),timeMillisSpend,localSearchSolverScope.getBestScore(),averageCalculateCountPerSecond});
}
