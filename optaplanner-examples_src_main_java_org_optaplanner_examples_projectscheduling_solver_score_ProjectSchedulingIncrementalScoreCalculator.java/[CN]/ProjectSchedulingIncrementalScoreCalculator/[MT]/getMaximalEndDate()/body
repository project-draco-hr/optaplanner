{
  int max=0;
  for (  Set<Allocation> allocations : allocationsPerProjectMap.values()) {
    for (    Allocation allocation : allocations) {
      Integer otherEndDate=allocation.getEndDate();
      if (otherEndDate != null) {
        max=Math.max(otherEndDate,max);
      }
    }
  }
  return max;
}
