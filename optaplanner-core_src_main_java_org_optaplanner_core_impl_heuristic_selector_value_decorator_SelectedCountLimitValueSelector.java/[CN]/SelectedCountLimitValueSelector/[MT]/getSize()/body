{
  if (!(childValueSelector instanceof EntityIndependentValueSelector)) {
    throw new IllegalArgumentException("To use the method getSize(), the moveSelector (" + this + ") needs to be based on a EntityIndependentValueSelector ("+ childValueSelector+ ")."+ " Check your @"+ ValueRangeProvider.class.getSimpleName()+ " annotations.");
  }
  long childSize=((EntityIndependentValueSelector)childValueSelector).getSize();
  return Math.min(selectedCountLimit,childSize);
}
