{
  for (  Assignment assignment : assignmentList) {
    if (!assignment.getEmployee().equals(fromEmployee)) {
      throw new IllegalStateException("The assignment (" + assignment + ") should have the same employee ("+ fromEmployee+ ") as the fromEmployee ("+ fromEmployee+ ").");
    }
    NurseRosterMoveHelper.moveEmployee(workingMemory,assignment,toEmployee);
  }
}
