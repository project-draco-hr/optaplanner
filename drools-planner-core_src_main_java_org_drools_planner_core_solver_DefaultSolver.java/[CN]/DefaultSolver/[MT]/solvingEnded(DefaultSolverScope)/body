{
  bestSolutionRecaller.solvingEnded(solverScope);
  ;
  long timeMillisSpend=solverScope.calculateTimeMillisSpend();
  if (timeMillisSpend == 0L) {
    timeMillisSpend=1L;
  }
  long averageCalculateCountPerSecond=solverScope.getCalculateCount() * 1000L / timeMillisSpend;
  logger.info("Solved: time spend ({}), best score ({}), average calculate count per second ({}).",new Object[]{timeMillisSpend,solverScope.getBestScore(),averageCalculateCountPerSecond});
}
