{
  if (!getCsvFile().exists()) {
    throw new IllegalStateException("The csvFile ( " + getCsvFile() + " ) of the statistic ( "+ getStatisticType()+ " ) of the single benchmark ( "+ solverProblemBenchmarkResult+ " ) doesn't exist.");
  }
 else   if (pointList != null) {
    throw new IllegalStateException("The pointList ( " + pointList + " ) of the statistic ( "+ getStatisticType()+ " ) of the single benchmark ( "+ solverProblemBenchmarkResult+ " ) should be null when unhibernating.");
  }
  initPointList();
  readCsvStatisticFile();
}
