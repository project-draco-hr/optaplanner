{
  for (  SolverBenchmark solverBenchmark : solverBenchmarkList) {
    solverBenchmark.benchmarkingEnded();
  }
  determineWinningResultScoreDelta();
  determineRanking();
  StatisticManager statisticManager=new StatisticManager(benchmarkInstanceDirectory.getName(),solverStatisticFilesDirectory,unsolvedSolutionFileToStatisticMap);
  statisticManager.writeStatistics(solverBenchmarkList);
}
