{
  super.stepEnded(customStepScope);
  CustomSolverPhaseScope customSolverPhaseScope=customStepScope.getCustomSolverPhaseScope();
  if (logger.isDebugEnabled()) {
    long timeMillisSpend=customSolverPhaseScope.calculateSolverTimeMillisSpend();
    logger.debug("    Step index ({}), time spend ({}), score ({}), {} best score ({}).",new Object[]{customStepScope.getStepIndex(),timeMillisSpend,customStepScope.getScore(),(customStepScope.getBestScoreImproved() ? "new" : "   "),customSolverPhaseScope.getBestScore()});
  }
}
