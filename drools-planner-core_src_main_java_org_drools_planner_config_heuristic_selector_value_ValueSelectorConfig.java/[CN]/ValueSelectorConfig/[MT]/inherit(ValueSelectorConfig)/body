{
  super.inherit(inheritedConfig);
  if (planningVariableName == null) {
    planningVariableName=inheritedConfig.getPlanningVariableName();
  }
  selectionOrder=ConfigUtils.inheritOverwritableProperty(selectionOrder,inheritedConfig.getSelectionOrder());
  cacheType=ConfigUtils.inheritOverwritableProperty(cacheType,inheritedConfig.getCacheType());
  valueProbabilityWeightFactoryClass=ConfigUtils.inheritOverwritableProperty(valueProbabilityWeightFactoryClass,inheritedConfig.getValueProbabilityWeightFactoryClass());
}
