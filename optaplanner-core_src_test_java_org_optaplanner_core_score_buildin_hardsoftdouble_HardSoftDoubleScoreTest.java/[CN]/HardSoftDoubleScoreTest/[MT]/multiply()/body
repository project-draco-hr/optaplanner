{
  assertEquals(HardSoftDoubleScore.valueOf(6.0,-6.0),HardSoftDoubleScore.valueOf(5.0,-5.0).multiply(1.2));
  assertEquals(HardSoftDoubleScore.valueOf(1.2,-1.2),HardSoftDoubleScore.valueOf(1.0,-1.0).multiply(1.2));
  assertEquals(HardSoftDoubleScore.valueOf(4.8,-4.8),HardSoftDoubleScore.valueOf(4.0,-4.0).multiply(1.2));
}
