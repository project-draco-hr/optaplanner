{
  Random workingRandom=mock(Random.class);
  when(workingRandom.nextInt(anyInt())).thenReturn(3,0,3,0,3,0,2,0);
  assertElementsOfIterator(createCompositeValueRange(Arrays.asList(0,2,5,10),Arrays.asList(-15,25,-1)).createRandomIterator(workingRandom),10,0);
  assertElementsOfIterator(createCompositeValueRange(Arrays.asList("a","b"),Arrays.asList("c"),Arrays.asList("d")).createRandomIterator(workingRandom),"d","a");
  assertElementsOfIterator(createCompositeValueRange(Collections.<String>emptyList(),Collections.<String>emptyList()).createRandomIterator(workingRandom));
}
