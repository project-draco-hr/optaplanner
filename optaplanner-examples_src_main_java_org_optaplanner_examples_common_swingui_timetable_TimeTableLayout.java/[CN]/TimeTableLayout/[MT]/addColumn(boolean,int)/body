{
  if (rows.size() > 0) {
    throw new IllegalStateException("Add all columns before adding rows");
  }
  int index=columns.size();
  Column column=new Column(index,autoWidth,baseWidth);
  columns.add(column);
  if (!autoWidth) {
    totalColumnWidth+=baseWidth;
  }
  cells.add(new ArrayList<Cell>());
  return index;
}
