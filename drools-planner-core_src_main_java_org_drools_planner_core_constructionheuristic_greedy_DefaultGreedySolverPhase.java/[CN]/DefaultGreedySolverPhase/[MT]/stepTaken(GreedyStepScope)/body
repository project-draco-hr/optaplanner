{
  super.stepTaken(greedyStepScope);
  greedyPlanningEntitySelector.stepTaken(greedyStepScope);
  logger.info("Step index ({}), time spend ({}), score ({}), initializing planning entities.",new Object[]{greedyStepScope.getStepIndex(),greedyStepScope.getGreedySolverPhaseScope().calculateTimeMillisSpend(),greedyStepScope.getScore()});
}
