{
  SolutionDescriptor<TestdataExtendedAbstractSolution> descriptor=new SolutionDescriptor<>(TestdataExtendedAbstractSolution.class);
  descriptor.processAnnotations(null);
  Assert.assertEquals("Fact collection inherited from abstract class is not registered.",1,descriptor.getFactCollectionPropertyAccessorMap().size());
  Assert.assertEquals("Private fact property from the solution class is not registered.",1,descriptor.getFactPropertyAccessorMap().size());
}
