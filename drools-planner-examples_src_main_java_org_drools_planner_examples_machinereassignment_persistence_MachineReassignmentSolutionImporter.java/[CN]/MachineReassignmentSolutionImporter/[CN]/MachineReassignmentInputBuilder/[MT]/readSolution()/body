{
  machineReassignment=new MachineReassignment();
  machineReassignment.setId(0L);
  readResourceList();
  readMachineList();
  readServiceList();
  readProcessList();
  readBalancePenaltyList();
  readGlobalPenaltyInfo();
  logger.info("MachineReassignment with {} resources.",new Object[]{machineReassignment.getResourceList().size()});
  return machineReassignment;
}
