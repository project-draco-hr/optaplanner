{
  if (this == o) {
    return true;
  }
 else   if (o instanceof HardSoftDoubleScore) {
    HardSoftDoubleScore other=(HardSoftDoubleScore)o;
    return initScore == other.getInitScore() && hardScore == other.getHardScore() && softScore == other.getSoftScore();
  }
 else {
    return false;
  }
}
