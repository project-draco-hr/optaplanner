{
  Class<?> entityClass=entityDescriptor.getEntityClass();
  for (  Class<?> otherEntityClass : entityDescriptorMap.keySet()) {
    if (entityClass.isAssignableFrom(otherEntityClass)) {
      throw new IllegalArgumentException("An earlier planningEntityClass (" + otherEntityClass + ") should not be a subclass of a later planningEntityClass ("+ entityClass+ "). Switch their declaration so superclasses are defined earlier.");
    }
  }
  entityDescriptorMap.put(entityClass,entityDescriptor);
  reversedEntityClassList.add(0,entityClass);
}
