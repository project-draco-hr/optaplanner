{
  SimpleLongScoreDefinition scoreDefinition=new SimpleLongScoreDefinition();
  SimpleLongScore pessimisticBound=scoreDefinition.buildPessimisticBound(InitializingScoreTrend.buildUniformTrend(InitializingScoreTrendLevel.ONLY_UP,1),SimpleLongScore.valueOf(-1L));
  assertEquals(-1L,pessimisticBound.getScore());
}
