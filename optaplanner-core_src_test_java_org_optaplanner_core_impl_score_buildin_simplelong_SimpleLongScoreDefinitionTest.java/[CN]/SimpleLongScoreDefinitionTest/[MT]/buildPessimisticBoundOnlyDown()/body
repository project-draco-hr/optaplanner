{
  SimpleLongScoreDefinition scoreDefinition=new SimpleLongScoreDefinition();
  SimpleLongScore pessimisticBound=scoreDefinition.buildPessimisticBound(InitializingScoreTrend.buildUniformTrend(InitializingScoreTrendLevel.ONLY_DOWN,1),SimpleLongScore.valueOf(-1L));
  assertEquals(Long.MIN_VALUE,pessimisticBound.getScore());
}
