{
  SimpleDoubleScoreDefinition scoreDefinition=new SimpleDoubleScoreDefinition();
  SimpleDoubleScore pessimisticBound=scoreDefinition.buildPessimisticBound(InitializingScoreTrend.buildUniformTrend(InitializingScoreTrendLevel.ONLY_UP,1),SimpleDoubleScore.valueOf(-1.7));
  assertEquals(-1.7,pessimisticBound.getScore(),0.0);
}
