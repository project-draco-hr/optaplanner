{
  Task aTask=a.getTask();
  Task bTask=b.getTask();
  return new CompareToBuilder().append(aTask.getResourceUsageMultiplicand(),bTask.getResourceUsageMultiplicand()).append(aTask.getPowerConsumptionMicros(),bTask.getPowerConsumptionMicros()).append(aTask.getDuration(),bTask.getDuration()).append(a.getId(),b.getId()).toComparison();
}
