{
  assertEquals(HardSoftBigDecimalScore.valueOf(new BigDecimal("6.0"),new BigDecimal("-6.0")),HardSoftBigDecimalScore.valueOf(new BigDecimal("5.0"),new BigDecimal("-5.0")).multiply(1.2));
  assertEquals(HardSoftBigDecimalScore.valueOf(new BigDecimal("1.2"),new BigDecimal("-1.2")),HardSoftBigDecimalScore.valueOf(new BigDecimal("1.0"),new BigDecimal("-1.0")).multiply(1.2));
  assertEquals(HardSoftBigDecimalScore.valueOf(new BigDecimal("4.8"),new BigDecimal("-4.8")),HardSoftBigDecimalScore.valueOf(new BigDecimal("4.0"),new BigDecimal("-4.0")).multiply(1.2));
}
