{
  assertEquals(scoreDefinitionHSS.createScore(5,-5,5),scoreDefinitionHSS.createScore(25,-25,25).divide(5.0));
  assertEquals(scoreDefinitionHSS.createScore(4,-5,4),scoreDefinitionHSS.createScore(21,-21,21).divide(5.0));
  assertEquals(scoreDefinitionHSS.createScore(4,-5,4),scoreDefinitionHSS.createScore(24,-24,24).divide(5.0));
}
