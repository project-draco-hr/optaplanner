{
  assertEquals(scoreDefinition.scoreValueOf(6,-6,6),scoreDefinition.scoreValueOf(5,-5,5).multiply(1.2));
  assertEquals(scoreDefinition.scoreValueOf(1,-2,1),scoreDefinition.scoreValueOf(1,-1,1).multiply(1.2));
  assertEquals(scoreDefinition.scoreValueOf(4,-5,4),scoreDefinition.scoreValueOf(4,-4,4).multiply(1.2));
}
