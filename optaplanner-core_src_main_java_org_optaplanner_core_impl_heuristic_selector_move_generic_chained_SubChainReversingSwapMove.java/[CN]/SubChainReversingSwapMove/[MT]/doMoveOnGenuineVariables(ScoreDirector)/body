{
  Object oldLeftValue=variableDescriptor.getValue(leftSubChain.getFirstEntity());
  Object oldRightValue=variableDescriptor.getValue(rightSubChain.getFirstEntity());
  ChainedMoveUtils.doReverseSubChainChange(scoreDirector,leftSubChain,variableDescriptor,oldRightValue == leftSubChain.getLastEntity() ? oldLeftValue : oldRightValue);
  ChainedMoveUtils.doReverseSubChainChange(scoreDirector,rightSubChain,variableDescriptor,oldLeftValue == rightSubChain.getLastEntity() ? leftSubChain.getFirstEntity() : oldLeftValue);
}
