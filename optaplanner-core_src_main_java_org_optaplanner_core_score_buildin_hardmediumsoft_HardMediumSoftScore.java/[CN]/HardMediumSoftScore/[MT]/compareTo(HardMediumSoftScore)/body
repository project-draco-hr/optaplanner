{
  if (hardScore != other.getHardScore()) {
    if (hardScore < other.getHardScore()) {
      return -1;
    }
 else {
      return 1;
    }
  }
 else {
    if (mediumScore != other.getMediumScore()) {
      if (mediumScore < other.getMediumScore()) {
        return -1;
      }
 else {
        return 1;
      }
    }
 else {
      if (softScore < other.getSoftScore()) {
        return -1;
      }
 else       if (softScore > other.getSoftScore()) {
        return 1;
      }
 else {
        return 0;
      }
    }
  }
}
