{
  GroupLayout layout=new GroupLayout(this);
  setLayout(layout);
  JPanel headerPanel=createHeaderPanel();
  JPanel computersPanel=createComputersPanel();
  layout.setHorizontalGroup(layout.createParallelGroup().addComponent(headerPanel).addComponent(computersPanel));
  layout.setVerticalGroup(layout.createSequentialGroup().addComponent(headerPanel,GroupLayout.PREFERRED_SIZE,GroupLayout.PREFERRED_SIZE,GroupLayout.PREFERRED_SIZE).addComponent(computersPanel,GroupLayout.PREFERRED_SIZE,GroupLayout.PREFERRED_SIZE,GroupLayout.PREFERRED_SIZE));
}
