{
  String[] lineTokens=line.split("\\|");
  if (lineTokens.length != numberOfTokens) {
    throw new IllegalArgumentException("Read line (" + line + ") is expected to contain "+ numberOfTokens+ " tokens seperated by a pipeline (|).");
  }
  for (int i=0; i < lineTokens.length; i++) {
    lineTokens[i]=lineTokens[i].trim();
  }
  return lineTokens;
}
