{
  if (inheritedList == null) {
    return originalList;
  }
 else   if (originalList == null) {
    return inheritedList;
  }
 else {
    List<T> mergedList=new ArrayList<T>(inheritedList);
    for (    T t : originalList) {
      if (!mergedList.contains(t)) {
        mergedList.add(t);
      }
    }
    return mergedList;
  }
}
