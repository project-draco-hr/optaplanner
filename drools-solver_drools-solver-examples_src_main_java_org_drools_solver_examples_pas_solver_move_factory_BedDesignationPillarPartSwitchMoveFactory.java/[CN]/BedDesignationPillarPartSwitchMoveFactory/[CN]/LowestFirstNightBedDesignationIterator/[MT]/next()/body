{
  lastNextWasLeft=nextIsLeft();
  BedDesignation lowest;
  if (lastNextWasLeft) {
    lowest=nextLeft;
    if (leftIterator.hasNext()) {
      nextLeft=leftIterator.next();
    }
 else {
      leftHasNext=false;
      nextLeft=null;
    }
  }
 else {
    lowest=nextRight;
    if (rightIterator.hasNext()) {
      nextRight=rightIterator.next();
    }
 else {
      rightHasNext=false;
      nextRight=null;
    }
  }
  return lowest;
}
