{
  super.phaseEnded(greedyFitSolverPhaseScope);
  greedyPlanningEntitySelector.phaseEnded(greedyFitSolverPhaseScope);
  greedyDecider.phaseEnded(greedyFitSolverPhaseScope);
  logger.info("Greedy phase ended at step index ({}) for best score ({}).",greedyFitSolverPhaseScope.getLastCompletedStepScope().getStepIndex(),greedyFitSolverPhaseScope.getBestScore());
}
