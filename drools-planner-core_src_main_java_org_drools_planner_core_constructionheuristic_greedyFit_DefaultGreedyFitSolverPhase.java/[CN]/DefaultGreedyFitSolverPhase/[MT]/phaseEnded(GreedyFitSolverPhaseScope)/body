{
  super.phaseEnded(greedyFitSolverPhaseScope);
  greedyPlanningEntitySelector.phaseEnded(greedyFitSolverPhaseScope);
  greedyDecider.phaseEnded(greedyFitSolverPhaseScope);
  logger.info("Phase constructionHeuristic ended: step total ({}), time spend ({}), best score ({}).",greedyFitSolverPhaseScope.getLastCompletedStepScope().getStepIndex() + 1,greedyFitSolverPhaseScope.calculateSolverTimeMillisSpend(),greedyFitSolverPhaseScope.getBestScore());
}
