  entitySorterClass=ConfigUtils.inheritOverwritableProperty(entitySorterClass,inheritedConfig.getEntitySorterClass());
  entityProbabilityWeightFactoryClass=ConfigUtils.inheritOverwritableProperty(entityProbabilityWeightFactoryClass,inheritedConfig.getEntityProbabilityWeightFactoryClass());
