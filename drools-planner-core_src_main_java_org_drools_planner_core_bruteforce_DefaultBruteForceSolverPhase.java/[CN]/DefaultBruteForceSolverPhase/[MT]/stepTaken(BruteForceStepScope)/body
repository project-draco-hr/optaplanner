{
  super.stepTaken(bruteForceStepScope);
  bruteForceEntityWalker.stepTaken(bruteForceStepScope);
  BruteForceSolverPhaseScope bruteForceSolverPhaseScope=bruteForceStepScope.getBruteForceSolverPhaseScope();
  logger.debug("    Step index ({}), time spend ({}), score ({}), {} best score ({}).",new Object[]{bruteForceStepScope.getStepIndex(),bruteForceSolverPhaseScope.calculateSolverTimeMillisSpend(),bruteForceStepScope.getScore(),(bruteForceStepScope.getBestScoreImproved() ? "new" : "   "),bruteForceSolverPhaseScope.getBestScore()});
}
