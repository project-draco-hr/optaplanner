{
  if (belowThreshold) {
    int newSize=list.size() + c.size();
    if (newSize > LIST_SIZE_THRESHOLD) {
      set=new LinkedHashSet<E>(newSize);
      set.addAll(list);
      list=null;
      belowThreshold=false;
      return set.addAll(c);
    }
 else {
      boolean changed=false;
      for (      E e : c) {
        if (!list.contains(e)) {
          changed=true;
          list.add(e);
        }
      }
      return changed;
    }
  }
 else {
    return set.addAll(c);
  }
}
