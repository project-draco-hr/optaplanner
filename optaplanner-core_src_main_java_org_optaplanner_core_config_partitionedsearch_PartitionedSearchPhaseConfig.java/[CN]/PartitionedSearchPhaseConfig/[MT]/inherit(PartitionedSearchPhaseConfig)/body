{
  super.inherit(inheritedConfig);
  threadPoolSize=ConfigUtils.inheritOverwritableProperty(threadPoolSize,inheritedConfig.getThreadPoolSize());
  solutionPartitionerClass=ConfigUtils.inheritOverwritableProperty(solutionPartitionerClass,inheritedConfig.getSolutionPartitionerClass());
  phaseConfigList=ConfigUtils.inheritMergeableListConfig(phaseConfigList,inheritedConfig.getPhaseConfigList());
}
