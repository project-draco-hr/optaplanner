{
  if (this == o) {
    return true;
  }
 else   if (o instanceof DoubleConstraintOccurrence) {
    DoubleConstraintOccurrence other=(DoubleConstraintOccurrence)o;
    return new EqualsBuilder().appendSuper(super.equals(other)).append(weight,other.weight).isEquals();
  }
 else {
    return false;
  }
}
